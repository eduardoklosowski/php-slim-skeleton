{
    "name": "eduardoklosowski/php-slim-skeleton",
    "type": "project",
    "description": "Estrutura de projeto PHP com Slim",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-pcre": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "php-di/php-di": "^6.3",
        "slim/php-view": "^3.0",
        "slim/psr7": "^1.2",
        "slim/slim": "^4.5",
        "symfony/dotenv": "^5.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "App/"
        }
    },
    "scripts": {
        "start": "php -S 127.0.0.1:8080 -t public"
    }
}
